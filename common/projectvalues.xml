<?xml version="1.0" encoding="utf-8"?>
<buildprojects>
  <project title="{your project title}">
    <essthreebucket>{the project s3 bucket}</essthreebucket>
    <targetstaticdir>{the top directory for all static content below}</targetstaticdir>
    <!-- The Path to Your AjaxMin might not be too bad -->
    <minifierdir>C:\Program Files (x86)\Microsoft\Microsoft Ajax Minifier\AjaxMin.exe</minifierdir>
    <!-- Currently I am updating the assembly 
         version with a Team City task, so I grab 
         that information for versioning the static js/css file title.
         Then my web app requests the latest files when deployed. 
         This will be eventually changed to not version each file each time and use a Team City build number-->
    <websiteassembly>{ASSEMBLY FOR GETTING VERSION}</websiteassembly>
  </project>
</buildprojects>